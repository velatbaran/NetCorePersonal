<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>@ViewData["Title"] - Welat BARAN</title>
    <meta content=".nercore,cookie-auth,sql,javascript,mvc,layer architicture,jquery,personal web" name="description">
    <meta content=".nercore,cookie-auth,sql,javascript,mvc,layer architicture,jquery,personal web" name="keywords">

    <!-- Favicons -->
    <link href="web-page/assets/img/favicon.png" rel="icon">
    <link href="web-page/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Vendor CSS Files -->
    <link href="web-page/assets/vendor/aos/aos.css" rel="stylesheet">
    <link href="web-page/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/lib/font-awesome/css/all.min.css" rel="stylesheet" />
    <link href="~/lib/toastr.js/toastr.css" rel="stylesheet" />
    <link href="web-page/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="web-page/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="web-page/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="web-page/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="web-page/assets/css/style.css" rel="stylesheet">

</head>

<body>

    <!-- ======= Mobile nav toggle button ======= -->
    <i class="bi bi-list mobile-nav-toggle d-xl-none"></i>

    <!-- ======= Header ======= -->
    <header id="header">
        <div class="d-flex flex-column">

            <partial name="_SocialPartial" />

            <nav id="navbar" class="nav-menu navbar">
                <ul>
                    <li><a href="#hero" class="nav-link scrollto active"><i class="bx bx-home"></i> <span>Home</span></a></li>
                    <li><a href="#about" class="nav-link scrollto"><i class="bx bx-user"></i> <span>About</span></a></li>
                    <li><a href="#resume" class="nav-link scrollto"><i class="bx bx-file-blank"></i> <span>Resume</span></a></li>
                    <li><a href="#portfolio" class="nav-link scrollto"><i class="bx bx-book-content"></i> <span>Portfolio</span></a></li>
                    <li><a href="#services" class="nav-link scrollto"><i class="bx bx-server"></i> <span>Services</span></a></li>
                    <li><a href="#contact" class="nav-link scrollto"><i class="bx bx-envelope"></i> <span>Contact</span></a></li>
                </ul>
            </nav><!-- .nav-menu -->
        </div>
    </header><!-- End Header -->
    <!-- ======= Hero Section ======= -->
    <section id="hero" class="d-flex flex-column justify-content-center align-items-center">
        <div class="hero-container" data-aos="fade-in">
            <h1>Velat BARAN</h1>
            <p>I'm <span class="typed" data-typed-items="Designer, Developer, Freelancer"></span></p>
        </div>
    </section><!-- End Hero -->

    <main id="main">

        @RenderBody()

    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <footer id="footer">
        <div class="container">
            <div class="copyright">
                &copy; Copyright <strong><span>Welat BARAN</span></strong>
            </div>
            <div class="credits">
                Designed by <a href="#">WBSoftware</a>
            </div>
        </div>
    </footer><!-- End  Footer -->

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="web-page/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="web-page/assets/vendor/aos/aos.js"></script>
    <script src="web-page/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/toastr.js/toastr.min.js"></script>
    <script src="web-page/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="web-page/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="web-page/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="web-page/assets/vendor/typed.js/typed.min.js"></script>
    <script src="web-page/assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="web-page/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="web-page/assets/js/main.js"></script>

</body>

</html>